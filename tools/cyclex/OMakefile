OBJECTS[] =
  compact.o
  lexer.o
  lexgen.o
  main.o
  output.o
  parser_tab.o
  syntax.o

boot ()

cyclex: $(OBJECTS) $(LIBCYC)
  cc -m32 $(OBJECTS) -o $@ -lcyc $(LDFLAGS)

.SUBDIRS: $(bootname .)
  cyclex.boot: $(OBJECTS) $(LIBCYC_BOOT)
    cc -m32 $(OBJECTS) -o $@ -lcyc.boot $(LDFLAGS_BOOT)

.DEFAULT: cyclex
