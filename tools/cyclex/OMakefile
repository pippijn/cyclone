OBJECTS[] =
  compact.o
  lexer.o
  lexgen.o
  main.o
  output.o
  parser_tab.o
  syntax.o

boot ()

cyclex: $(OBJECTS) $(LIBCYC)
  cc -m32 $(OBJECTS) -o $@ -lcyc $(LDFLAGS)

cyclex.boot: $(bootname $(OBJECTS)) $(LIBCYC_BOOT)
  cc -m32 $(bootname $(OBJECTS)) -o $@ -lcyc.boot $(LDFLAGS)

.DEFAULT: cyclex
