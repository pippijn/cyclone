OBJECTS[] =
  LR0.o
  closure.o
  conflicts.o
  derives.o
  files.o
  getargs.o
  gram.o
  lalr.o
  lex.o
  main.o
  nullable.o
  output.o
  print.o
  reader.o
  reduce.o
  symtab.o
  version.o
  warshall.o
  cycbison_simple.string.o

boot ()

cycbison: $(OBJECTS) $(LIBCYC)
  cc -m32 -o $@ $(OBJECTS) -lcyc $(LDFLAGS)

cycbison.boot: $(bootname $(OBJECTS)) $(LIBCYC_BOOT)
  cc -m32 -o $@ $(bootname $(OBJECTS)) -lcyc.boot $(LDFLAGS)

.DEFAULT: cycbison
